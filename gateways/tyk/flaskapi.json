{
    "name": "Flask API",
    "slug": "flask-api",
    "api_id": "flaskapi",
    "org_id": "flaskapi",
    "use_keyless": true,
    "auth": {
      "auth_header_name": "Authorization"
    },
    "definition": {
      "location": "header",
      "key": "x-api-version"
    },
    "version_data": {
      "not_versioned": true,
      "versions": {
        "Default": {
          "name": "Default",
          "use_extended_paths": true
        }
      }
    },
    "proxy": {
      "listen_path": "/flaskapi/",
      "target_url": "http://flaskapi:8080/",
      "strip_listen_path": true
    },
    "active": true
}